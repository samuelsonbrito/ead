        "jquery": "^3.2",
        "laravel-mix": "^2.0",
        "lodash": "^4.17.10",
        "popper.js": "^1.14.3",
        "vue": "^2.5.16",
        "vue-router": "^3.0.1",
        "vuex": "^3.0.1"
    },
    "dependencies": {
